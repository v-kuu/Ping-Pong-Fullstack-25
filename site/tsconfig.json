{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "moduleResolution": "bundler",
    "types": ["astro/client"]
  },
  "include": ["src/**/*", "components/**/*", "islands/**/*"],
  "exclude": ["node_modules", "dist"]
}
